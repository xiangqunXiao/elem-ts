{
  "name": "vue-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.vue  --config ./configs/.eslintrc  --ignore-path ./configs/.eslintignore --quiet --fix",
    "lint:format": "prettier --config ./configs/.prettierrc --loglevel warn --write \"./**/*.{js,jsx,ts,vue,css,md,json}\" --ignore-path ./configs/.prettierignore && prettier --write ./README.md ",
    "lint": "pnpm lint:format && pnpm lint:fix"
  },
  "dependencies": {
    "element-plus": "^2.2.0",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.7",
    "vue-tsc": "^0.34.7",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "prettier": "^2.5.1"
  }
}
